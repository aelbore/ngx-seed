{
  "paths": {
    "npm:": "node_modules/",
    "rxjs/operators": "node_modules/rxjs/operators/index.js"
  },
  "map": {
    "app":                        "app",
    "dist":                       "dist",
    "tslib":                      "npm:tslib"
  },
  "packages": {
    "app":                                { "main": "index.js",  "defaultExtension": "js" },
    "dist":                               { "main": "index.js", "defaultExtension": "js" },
    "tslib":                              { "main": "tslib.js", "defaultExtension": "js" }
  }   
}