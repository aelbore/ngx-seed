{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome",
      "url": "http://localhost:4000",
      "port": 4000,
      "sourceMaps": true,
      "webRoot": "${workspaceFolder}/dist/",
      "sourceMapPathOverrides": {
        "../.tmp/app/*": "${workspaceRoot}/.tmp/app/*"
      }
    }
  ]
}